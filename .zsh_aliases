# Shortcuts
alias c='clear'
alias :q='exit'
alias ll='ls -al'

# Utils
alias reload='clear && source ~/.zshrc'
alias diff='git diff --no-index'
alias flushdns='sudo dscacheutil -flushcache;sudo killall -HUP mDNSResponder'

# Services
alias pastebin='nc virtualhacker.net 9999'
alias exifconfig='curl ifconfig.co'

# Pre-configured Screens
alias sacha-start='screen -c ~/.screen/sacha'
alias stylus-start='screen -c ~/.screen/stylus'
alias atom=atom-beta
